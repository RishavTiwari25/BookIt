{
  "name": "server",
  "version": "1.0.0",
  "description": "BookIt API server",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
  "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
  "seed": "tsx prisma/seed.ts",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": ["express","prisma","postgres","typescript"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.2",
    "prisma": "^6.18.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3"
  }
}
